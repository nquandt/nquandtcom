---
interface Props {
    title: string;
    time: string;
    location: string;
    company: string;
}

const { title, time, company, location } = Astro.props;
---

<div class="flex justify-between">
    <div class="font-bold">
        {title}
    </div>
    <div>
        {time}
    </div>
</div>
<div class="flex justify-between">
    <div>
        {company}
    </div>
    <div>
        {location}
        <!-- {time} -->
    </div>
</div>
